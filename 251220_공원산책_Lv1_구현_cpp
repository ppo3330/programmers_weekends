//O(park + routes)
//구현이 어렵지는 않았으나 string으로 입력받은 int값에 대해서 -'0'을 안해줘서 논리 오류 발생
//map과 dir을 이용하여 방향과 움직임을 간소화 하는 방법도 있음.
#include <string>
#include <vector>

using namespace std;

vector<int> solution(vector<string> park, vector<string> routes) {
    vector<int> answer(2);
    int H = park.size();
    int W = park[0].size();
    for(int i=0;i<H;i++){
        for(int j=0;j<W;j++){
            if(park[i][j] == 'S'){
            answer[0] = i;
            answer[1] = j;
            }
        }
    }
    
    for(int i=0;i<routes.size();i++){
        char op = routes[i][0];
        int n = routes[i][2]-'0';
        int x= answer[0];
        int y = answer[1];
        bool gogo = true;
        if (op == 'N'){
            if(x-n < 0){ 
                gogo = false;
                continue;
                       }
            for(int j=1;j<=n;j++){
                if(park[x-j][y] == 'X'){
                    gogo = false;
                    break;
                }
            }
            if(gogo == true)
                answer[0] -= n;
        }
        else if (op == 'S'){
            if(x +n >= H){ 
                gogo = false;
                continue;
                       }
            for(int j=1;j<=n;j++){
                if(park[x+j][y] == 'X'){
                    gogo = false;
                    break;
                }
            }
            if(gogo == true)
                answer[0] +=n;
        }
        else if (op == 'W'){
            if(y - n < 0){ 
                gogo = false;
                continue;
                       }
            for(int j=1;j<=n;j++){
                if(park[x][y-j] == 'X'){
                    gogo = false;
                    break;
                }
            }
            if(gogo == true)
                answer[1] -=n;
        }
        else{
            if(y + n >= W){ 
                gogo = false;
                continue;
                       }
            for(int j=1;j<=n;j++){
                if(park[x][y+j] == 'X'){
                    gogo = false;
                    break;
                }
            }
            if(gogo == true)
                answer[1] +=n;
        }
    }
    
    return answer;
}
