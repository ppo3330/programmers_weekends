O(goal)
#include <string>
#include <vector>

using namespace std;

string solution(vector<string> cards1, vector<string> cards2, vector<string> goal) {
    string answer = "Yes";
    
    int cnt1 = 0;
    int cnt2 = 0;
    
    for (int i=0;i<goal.size();i++){
        if (goal[i] == cards1[cnt1]){
            if(cnt1 == cards1.size()-1) continue;
            cnt1++;
            continue;
        }
        else if (goal[i] == cards2[cnt2]){
            if(cnt2 == cards2.size()-1) continue;
            cnt2++;
            continue;
        }
        else{
            answer = "No";
            break;
        }
    }
    return answer;
}
